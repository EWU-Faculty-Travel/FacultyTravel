USE travel_app;

CREATE TABLE `tbl_trip` (
  `trip_id` int(11) NOT NULL AUTO_INCREMENT,
  `dept_code` char(4) NOT NULL,
  `trip_name` varchar(50) NOT NULL,
  `user_id` int(11) NOT NULL,
  `start_date` datetime DEFAULT NULL,
  `end_date` datetime DEFAULT NULL,
  `departure` varchar(50) DEFAULT NULL,
  `destination` varchar(50) DEFAULT NULL,
  `purpose` varchar(250) DEFAULT NULL,
  `group_count` int(11) DEFAULT '0',
  `authorization` char(8) DEFAULT NULL,
  `status_code` int(1) NOT NULL DEFAULT '0',
  PRIMARY KEY (`trip_id`),
  KEY `dept_code` (`dept_code`),
  KEY `user_id` (`user_id`),
  KEY `status_code` (`status_code`),
  CONSTRAINT `tbl_trip_ibfk_1` FOREIGN KEY (`dept_code`) REFERENCES `tbl_departments` (`dept_code`),
  CONSTRAINT `tbl_trip_ibfk_2` FOREIGN KEY (`user_id`) REFERENCES `tbl_users` (`user_id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `tbl_trip_ibfk_3` FOREIGN KEY (`status_code`) REFERENCES `tbl_status` (`status_code`) ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=357 DEFAULT CHARSET=latin1;


